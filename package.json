{
  "name": "ps-redux",
  "description": "React and Redux Pluralsight course by Cory House",
  "license": "MIT",
  "scripts": {
    "start": "run-p start:server start:client",
    "start:client": "webpack-dev-server --env.ENVIRONMENT=development --config webpack.config.dev.js --port 3000",
    "prestart:server": "node tools/createMockDb.js",
    "start:server": "node tools/apiServer.js",
    "test": "jest --watch",
    "clean:build": "rimraf ./build & mkdir build",
    "test:ci": "jest",
    "prebuild": "run-p clean:build test:ci",
    "build": "webpack --env.ENVIRONMENT=production  --config webpack.config.prod.js",
    "postbuild": "run-p start:server serve:build",
    "serve:build": "http-server ./build -a localhost -p 3000"
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "auth0-js": "^9.8.0",
    "auth0-lock": "^11.10.0",
    "axios": "^0.19.0",
    "bootstrap": "4.3.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "immer": "8.0.1",
    "jwks-rsa": "^1.3.0",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-toastify": "^5.4.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.12",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-toastify": "^4.1.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "dotenv": "^8.1.0",
    "ecstatic": "^2.2.2",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.15.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.9.0",
    "jest": "24.5.0",
    "json-server": "0.14.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "jest": {
    "setupFiles": [
      "./test/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/test/styleMock.js"
    }
  }
}
